<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::outlet_elements" title="Lof Outlet">
                    <resource id="Lof_Outlet::outlet" title="Outlets" sortOrder="10">
                        <resource id="Lof_Outlet::outlet_edit" title="Edit Outlet" sortOrder="10" />
                        <resource id="Lof_Outlet::outlet_save" title="Save Outlet" sortOrder="20" />
                        <resource id="Lof_Outlet::outlet_delete" title="Delete Outlet" sortOrder="30" />
                        <resource id="Lof_Outlet::outlet_view" title="View Outlet" sortOrder="40" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Lof_Outlet::config_outlet" title="Outlet" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
